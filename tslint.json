{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "align": [],
    "no-console": [
      true,
      "log"
    ],
    "semicolon": [
      true,
      "always",
      "strict-bound-class-methods"
    ],
    "no-var-requires": false
  },
  "rulesDirectory": []
}
