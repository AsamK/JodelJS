{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "max-line-length": [
      true,
      {
        "limit": 140,
        "ignore-pattern": "^import |^export {(.*?)}"
      }
    ],
    "no-console": [
      true,
      "log"
    ],
    "ordered-imports": [
      true,
      {
        "groups": [
          "^[^.]",
          "^\\."
        ],
        "grouped-imports": true
      }
    ],
    "semicolon": [
      true,
      "always",
      "strict-bound-class-methods"
    ],
    "typedef": [
      true,
      "call-signature",
      "property-declarations"
    ],
    "whitespace": [
      true,
      "check-module"
    ],
    "no-empty-interface": false,
    "no-var-requires": false
  },
  "rulesDirectory": []
}
